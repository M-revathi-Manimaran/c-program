#include <stdio.h>

/* Function to calculate average */
float calculateAverage(int a, int b, int c) {
    return (a + b + c) / 3.0;
}

int main() {
    int n1, n2, n3;
    float average;

    printf("Enter three numbers: ");
    scanf("%d", &n1);
    scanf("%d", &n2);
    scanf("%d", &n3);

    average = calculateAverage(n1, n2, n3);

    printf("Average = %.2f\n", average);

    return 0;
}
