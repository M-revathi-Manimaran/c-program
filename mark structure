#include <stdio.h>

/* Structure to store student marks */
struct Student {
    int rollNo;
    char name[50];
    int marks[5];   // marks of 5 subjects
    int total;
};

int main() {
    int i, j;
    struct Student s;

    printf("Enter student roll number: ");
    scanf("%d", &s.rollNo);

    printf("Enter student name: ");
    scanf(" %[^\n]s", s.name);

    printf("Enter marks of 5 subjects:\n");
    s.total = 0;
    for (j = 0; j < 5; j++) {
        printf("Subject %d: ", j + 1);
        scanf("%d", &s.marks[j]);
        s.total += s.marks[j];
    }

    printf("\n--- Student Result ---\n");
    printf("Roll No : %d\n", s.rollNo);
    printf("Name    : %s\n", s.name);
    printf("Total Marks: %d\n", s.total);

    return 0;
}
