#include <stdio.h>
#include <string.h>

/* Structure to store login credentials */
struct Login {
    char username[20];
    char password[20];
};

int main() {
    struct Login stored, entered;

    /* Store credentials */
    strcpy(stored.username, "admin");
    strcpy(stored.password, "12345");

    /* User input */
    printf("Enter username: ");
    scanf("%s", entered.username);

    printf("Enter password: ");
    scanf("%s", entered.password);

    /* Verify login */
    if (strcmp(stored.username, entered.username) == 0 &&
        strcmp(stored.password, entered.password) == 0) {
        printf("\nLogin Successful!\n");
    } else {
        printf("\nInvalid Username or Password\n");
    }

    return 0;
}
